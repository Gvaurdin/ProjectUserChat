<Application x:Class="WpfAppUsersChat.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfAppUsersChat"
             StartupUri="MVVM/View/WindowVerification.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./Themes/ContactsCard.xaml"/>
                <ResourceDictionary Source="./Themes/ChatItems.xaml"/>
                <ResourceDictionary Source="./Themes/MessageBox.xaml"/>
                <ResourceDictionary Source="./Themes/StylishScrollbar.xaml"/>
                <ResourceDictionary Source="./Themes/ButtonHoverEffect.xaml"/>
                <ResourceDictionary Source="./Themes/OvalFormTextBox.xaml"/>
                <ResourceDictionary Source="./Themes/OvalFormPasswordBox.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
